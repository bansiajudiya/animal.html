    if (!window.location.pathname.startsWith('/profile/orders/shipment')) return; // Ensure correct route

    document.querySelectorAll('.link-buttons').forEach(el => {
        if (el && el.innerText && el.innerText.trim().toUpperCase() === 'RETURN') {
            el.remove();
        }
    });
}

// MutationObserver to watch for elements appearing dynamically
function observeDOMChanges() {
    const observer = new MutationObserver(() => {
        removeCancelButtons(); // Run whenever the DOM updates
    });

    observer.observe(document, { childList: true, subtree: true }); // Observe all changes
}

// Function to handle route changes (for SPAs)
function handleRouteChange() {
    setTimeout(() => {
        if (window.location.pathname.startsWith('/profile/orders/shipment')) {
            removeCancelButtons(); // Remove immediately if present
            observeDOMChanges(); // Start observing for future additions
        }
    }, 500); // Delay ensures dynamic elements are detected
}

// Detect route changes in SPA
const pushState = history.pushState;
history.pushState = function () {
    pushState.apply(history, arguments);
    handleRouteChange();
};
window.addEventListener("popstate", handleRouteChange);
window.addEventListener("hashchange", handleRouteChange);

// Run the script when the page loads
handleRouteChange();
